{
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "printWidth": 200,
        "tabWidth": 2,
        "useTabs": false,
        "singleQuote": false,
        "bracketSpacing": true,
        "explicitTypes": "preserve"
      }
    },
    {
      "files": "*.jsx",
      "options": {
        "printWidth": 200,
        "tabWidth": 2,
        "useTabs": false,
        "singleQuote": false,
        "bracketSpacing": true,
        "explicitTypes": "preserve"
      }
    }
  ]
}
